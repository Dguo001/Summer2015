#include <iostream> 
#include "strings"

using namespace std;

int main () {
	string item1, item2, item3, item4, item5;
	cout << "This is a five item to-do list.  WHat is your first item?" <<endl;
	getline (cin, item1);
	cout <<endl <<endl <<endl << "Item 1 is " << item1 << " . What is item 2?" <<endl;
	getline (cin, item2);
	cout <<endl <<endl <<endl << "Item 1 is " << item1 << " . Item 2 is " << item2 << " .  What is item 3?" <<endl; 
	getline (cin, item3);
	cout <<endl <<endl <<endl << "Item 1 is " << item1 << " . Item 2 is " << item2 << " . Item 3 is " << item3 << " . What is item 4?" <<endl;
	getline (cin, item4);
	cout <<endl <<endl <<endl << "Item 1 is " << item1 << " . Item 2 is " << item2 << " . Item 3 is " << item3 << " . Item 4 is " << item4 << " . What is item 5?" <<endl;
	getline (cin, item5);
	cout <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl <<endl << "These are your list items." <<endl;
	cout << "1. " << item1 << "." <<endl;
	cout << "2. " << item2 << "." <<endl;
	cout << "3. " << item3 << "." <<endl;
	cout << "4. " << item4 << "." <<endl;
	cout << "5. " << item5 << "." <<endl <<endl <<endl;
	cout << "You're welcome humanity!" <<endl;
	cout << ":)" <<endl;
	return 0;  
}
