#include <iostream> 
#include <string>

using namespace std;

int main () {
	int boxSize;
	string list;
	cout << "Hello user.  How big of a list do you want to make?" <<endl <<endl.
	cin >> boxSize;
	string box [boxSize];
	for ( int b = 0; b <= boxSize; b = b+1)
	{
		cout << "What do you want to put in your list?" <<endl <<endl;
		getline (cin, list);
		box [b] = list;
		printarray(box, BoxSize);
	}
	return 0;  
}
